<UserControl x:Class="ThanksCard.Modules.LogonCopyright.Views.LogonCopyright"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <TextBlock>
            <Run Text="© 2019 XXX Corporation" FontSize="10"/>
            <Hyperlink x:Name="help" Command="{Binding HelpCommand}">ヘルプ</Hyperlink>
        </TextBlock>
    </Grid>
</UserControl>
